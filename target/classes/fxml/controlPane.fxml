<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx/14.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="dev.dawidlachor.controller.ControlPaneController">
    <children>
        <HBox alignment="CENTER_LEFT">
            <Button id="prev" fx:id="previousButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0"
                    HBox.hgrow="NEVER"/>
            <ToggleButton id="play" fx:id="playButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0"
                          HBox.hgrow="NEVER"/>
            <Button id="next" fx:id="nextButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="30.0"
                    HBox.hgrow="NEVER"/>
            <Slider fx:id="volumeSlider" HBox.hgrow="NEVER">
                <padding>
                    <Insets left="10.0" right="10.0"/>
                </padding>
            </Slider>
            <Slider fx:id="progressSlider" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets left="10.0" right="10.0"/>
                </padding>
            </Slider>
        </HBox>
        <TextField fx:id="messageTextField" alignment="CENTER" text="Mp3Player v1.0" editable="false"/>
    </children>
</VBox>